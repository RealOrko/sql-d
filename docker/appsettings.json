{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "System": "Error",
      "Default": "Error",
      "Microsoft": "Error"
    }
  },
  "SqlD": {
    "loglevel": "info",
    "enabled": true,
    "datadirectory": "/var/lib/sql-d",
    "settings": {
      "connections": {
        "strategy": "singleton"
      },
      "forwarding": {
        "allowed": true,
        "strategy": "secondary"
      },
      "replication": {
        "allowed": true,
        "interval": 30,
        "delay": 30
      }
    },
    "registries": [
      {
        "host": "localhost",
        "port": 50095
      }
    ],
    "services": [
      {
        "name": "registry",
        "database": "registry.sqld",
        "host": "localhost",
        "port": 50095,
        "tags": [
          "registry"
        ],
        "pragma": {
          "journalMode": "OFF",
          "synchronous": "OFF",
          "tempStore": "OFF",
          "lockingMode": "OFF",
          "countChanges": "OFF",
          "pageSize": "65536",
          "cacheSize": "10000",
          "queryOnly": "OFF",
          "autoVacuum": "INCREMENTAL",
          "autoVacuumPages": "64"
        }
      },
      {
        "name": "master",
        "database": "master.sqld",
        "host": "localhost",
        "port": 50100,
        "tags": [
          "master"
        ],
        "pragma": {
          "journalMode": "OFF",
          "synchronous": "OFF",
          "tempStore": "OFF",
          "lockingMode": "OFF",
          "countChanges": "OFF",
          "pageSize": "65536",
          "cacheSize": "10000",
          "queryOnly": "OFF",
          "autoVacuum": "INCREMENTAL",
          "autoVacuumPages": "64"
        },
        "forwardingTo": [
        ]
      }
    ]
  }
}