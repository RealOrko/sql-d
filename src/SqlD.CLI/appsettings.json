{
  "Logging": {
    "LogLevel": {
      "System": "Error",
      "Default": "Error",
      "Microsoft": "Error"
    }
  },
  "SqlD": {
    "loglevel": "info",
    "enabled": true,
    "settings": {
      "connections": {
        "strategy": "singleton"
      },
      "forwarding": {
        "allowed": true,
        "strategy": "secondary"
      },
      "replication": {
        "allowed": true,
        "interval": 30,
        "delay": 30
      }
    },
    "registries": [],
    "services": [
      {
        "name": "sql-d-master-1",
        "database": "sql-d-master-1.db",
        "host": "localhost",
        "port": 50110,
        "tags": [
          "master"
        ],
        "pragma": {
          "journalMode": "OFF",
          "synchronous": "OFF",
          "tempStore": "OFF",
          "lockingMode": "OFF",
          "countChanges": "OFF",
          "pageSize": "65536",
          "cacheSize": "10000",
          "queryOnly": "OFF",
          "autoVacuum": "INCREMENTAL",
          "autoVacuumPages": "64"
        },
        "forwardingTo": []
      }
    ]
  }
}