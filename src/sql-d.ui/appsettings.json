{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "System": "Error",
      "Default": "Error",
      "Microsoft": "Error"
    }
  },
  "SqlD": {
    "enabled": true,
    "settings": {
      "forwarding": {
        "allowed": false,
        "strategy": "secondary"
      },
      "replication": {
        "allowed": true,
        "interval": 30
      }
    },
    "registries": [
      {
        "host": "localhost",
        "port": 50095
      }
    ],
    "services": [
      {
        "name": "registry-1",
        "database": "registry-1.db",
        "host": "localhost",
        "port": 50095,
        "tags": [
          "registry"
        ],
        "pragma": {
          "journalMode": "OFF",
          "synchronous": "OFF",
          "tempStore": "OFF",
          "lockingMode": "OFF",
          "countChanges": "OFF",
          "pageSize": "65536",
          "cacheSize": "10000",
          "queryOnly": "OFF"
        }
      },
      {
        "name": "slave-1",
        "database": "slave-1.db",
        "host": "localhost",
        "port": 50101,
        "tags": [
          "slave 1"
        ],
        "pragma": {
          "journalMode": "OFF",
          "synchronous": "OFF",
          "tempStore": "OFF",
          "lockingMode": "OFF",
          "countChanges": "OFF",
          "pageSize": "65536",
          "cacheSize": "10000",
          "queryOnly": "OFF"
        },
        "forwardingTo": [
          {
            "host": "localhost",
            "port": 50102
          }
        ]
      },
      {
        "name": "slave-2",
        "database": "slave-2.db",
